Module Zad2_bezunosa
    Sub Main()
        Dim prviBroj As Short
        Dim drugiBroj As Short
        prviBroj = 5
        drugiBroj = 12
        Console.WriteLine("Osnovne aritmetičke operacije")
        Console.WriteLine("-----------------------------")
        Console.WriteLine("{0}+{1}={2}", prviBroj, drugiBroj, prviBroj + drugiBroj)
        Console.WriteLine("{0}-{1}={2}", prviBroj, drugiBroj, prviBroj - drugiBroj)
        Console.WriteLine("{0}*{1}={2}", prviBroj, drugiBroj, prviBroj * drugiBroj)
        Console.WriteLine("{0}/{1}={2}", prviBroj, drugiBroj, prviBroj / drugiBroj)
        Console.ReadLine()
    End Sub
End Module
